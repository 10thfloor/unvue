{
  "name": "unvue",
  "version": "0.2.3",
  "description": "unopinionated, universal Vue.js app made simple.",
  "repository": {
    "url": "egoist/unvue",
    "type": "git"
  },
  "main": "lib/index.js",
  "files": [
    "lib",
    "app"
  ],
  "scripts": {
    "test:cov": "jest --coverage && npm run lint",
    "test": "jest && npm run lint",
    "lint": "xo",
    "precommit": "npm t"
  },
  "author": "egoist <0x142857@gmail.com>",
  "license": "MIT",
  "jest": {
    "testEnvironment": "node"
  },
  "devDependencies": {
    "eslint-config-rem": "^3.0.0",
    "express": "^4.15.2",
    "husky": "^0.13.2",
    "jest-cli": "^18.1.0",
    "xo": "^0.17.1"
  },
  "xo": {
    "extends": "rem",
    "esnext": true,
    "envs": [
      "jest"
    ],
    "ignores": [
      "example/**/*",
      "app/**/*"
    ],
    "rules": {
      "guard-for-in": 0,
      "import/no-dynamic-require": 0
    }
  },
  "dependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-preset-vue-app": "^0.5.0",
    "chalk": "^1.1.3",
    "compression": "^1.6.2",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.27.3",
    "es6-promise": "^4.1.0",
    "eventsource-polyfill": "^0.9.6",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "lru-cache": "^4.0.2",
    "memory-fs": "^0.4.1",
    "postcss-loader": "^1.3.3",
    "rimraf": "^2.6.1",
    "serialize-javascript": "^1.3.0",
    "style-loader": "^0.14.1",
    "vue": "^2.2.4",
    "vue-loader": "^11.1.4",
    "vue-meta": "^0.5.5",
    "vue-router": "^2.3.0",
    "vue-server-renderer": "^2.2.4",
    "vue-ssr-webpack-plugin": "^1.0.2",
    "vue-template-compiler": "^2.2.4",
    "vuex": "^2.2.1",
    "vuex-router-sync": "^4.1.2",
    "webpack": "^2.2.1",
    "webpack-chain": "^3.0.0",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1",
    "webpack-node-externals": "^1.5.4"
  },
  "peerDependencies": {
    "express": "^4.15.2"
  }
}
